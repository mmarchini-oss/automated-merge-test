{
  "problemMatcher": [
    {
      "owner": "js-tests",
      "pattern": [
        {
          "regexp": "^Absolute Path: (.*):(\\d+):(\\d)$",
          "file": 1,
          "line": 2,
          "column": 3
        },
        {
          "regexp": "(^>.*$\n)*",
          "message": 1
        }
      ]
    }
  ]
}
